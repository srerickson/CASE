:css

  .d3_vis {
    margin-top:50px;
    background-color:#eee;
    border:1px solid #bbb;

  }

  .axis path,
  .axis line {
      fill: none;
      stroke: black;
      shape-rendering: crispEdges;
  }

  .axis text {
      font-family: sans-serif;
      font-size: 11px;
  }


.span X-Axis
%select.question_analysis.x_axis
  - @results.each_with_index do |r,i|
    %option{:value => i}
      = "#{r.question.id}. #{r.question.to_s}"

.span Y-Axis
%select.question_analysis.y_axis
  - @results.each_with_index do |r,i|
    %option{:value => i}
      = "#{r.question.id}. #{r.question.to_s}"



.d3_vis{:style=>"width:1100px;height:650px;"}


:javascript
  d3.select(".d3_vis").datum(#{@results.to_json})